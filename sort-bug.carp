(defn main []
  (let [in   [20 0 10 21 11 1 2 22 12 24 23 13 3 14 4 25 5 15 16 6 17 7 8 18 19 9]
        exp  (Array.range 0 25 1)
        heap (Array.copy &in)
        out  (Array.sorted &in)]
    (do
      (MaxHeap.heapify! &heap)

      (println* "in:   " &in)
      (println* "heap: " &heap)
      (println* "exp:  " &exp)
      (println* "out:  " &out)
      (println* "(= out exp): " (= &out &exp)))))
